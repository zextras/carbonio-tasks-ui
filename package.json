		"dotenv": "^16.0.3",
		"graphql": "^16.7.1",
		"i18next": "^22.5.1",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-i18next": "^12.2.0",
		"react-router-dom": "^5.3.4",
		"styled-components": "^5.3.9",
		"uuid": "^9.0.0"
	},
	"browserslist": [
		">1%",
		"last 1 version",
		"Firefox ESR",
		"not dead"
	],
	"carbonio": {
		"translations_repository": "git@github.com:Zextras/carbonio-tasks-ui-i18n.git",
		"name": "carbonio-tasks-ui",
		"priority": 10,
		"type": "carbonio",
		"display": "Tasks",
		"icon": "ListViewOutline"
	},
	"config": {
		"translations_repository": "git@github.com:Zextras/carbonio-tasks-ui-i18n.git"
	}
